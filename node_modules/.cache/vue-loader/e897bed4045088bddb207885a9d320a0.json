\n          <v-flex xs6 sm2 md1>\r\n\t\t\t\t\t\t<v-btn fab small flat class='ml-5' to='/projects'>\r\n\t\t\t\t\t\t\t<v-icon>open_in_new</v-icon>\r\n\t\t\t\t\t\t</v-btn>\r\n          </v-flex>\r\n        </v-layout>\r\n        <v-divider></v-divider>\r\n      </v-card>\r\n    </v-container>\r\n\r\n    <!-- Display Employees -->\r\n    <v-container class=\"my-3 text-capitalize\">\r\n      <h2 class='font-weight-light headline'>Groups</h2>\r\n      <!-- Sorting Buttons -->\r\n      <v-layout row>\r\n        <v-tooltip top>\r\n          <v-btn fab small flat color=\"tertiary\" slot=\"activator\">\r\n            <v-icon small>person</v-icon>\r\n          </v-btn>\r\n          <span>Sort by name</span>\r\n        </v-tooltip>\r\n        <v-tooltip top>\r\n          <v-btn fab small flat color=\"tertiary\" slot=\"activator\">\r\n            <v-icon small>build</v-icon>\r\n          </v-btn>\r\n          <span>Sort by role</span>\r\n        </v-tooltip>\r\n        <v-spacer></v-spacer>\r\n        <addGroup @groupAdded='groupSnackbar = true' />\r\n      </v-layout>\r\n      <!-- Content -->\r\n      <v-card flat v-for=\"p in groups\" :key=\"p.name\" class='py-2'>\r\n        <v-layout row wrap>\r\n          <v-flex xs12 sm2>\r\n            <v-responsive>\r\n              <v-avatar size='60' class='white'>\r\n                <!-- <img :src='p.avatar'> -->\r\n              </v-avatar>\r\n            </v-responsive>\r\n          </v-flex>\r\n          <v-flex xs6 sm5>\r\n            <div class=\"caption grey--text\">Name</div>\r\n            <div class='subheading'>{{ p.name }}</div>\r\n          </v-flex>\r\n          <!-- <v-flex xs6 sm4>\r\n            <div class=\"caption grey--text\">Role</div>\r\n            <div class='subheading'>{{ p. }}</div>\r\n          </v-flex> -->\r\n          <v-flex xs6 sm1>\r\n\t\t\t\t\t\t<v-btn fab small flat class='ma-0' to='/projects'>\r\n\t\t\t\t\t\t\t<v-icon>open_in_new</v-icon>\r\n\t\t\t\t\t\t</v-btn>\r\n          </v-flex>\r\n        </v-layout>\r\n        <v-divider></v-divider>\r\n      </v-card>\r\n    </v-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport addProject from '@/components/addProject.vue'\r\nimport addGroup from '@/components/addGroup.vue'\r\nimport {mapState} from 'vuex'\r\nimport {createGroup, getGroup, deleteGroup, updateGroup} from '../modules/groupController.js';\r\nimport {createProject, getProject, updateProject, deleteProject} from '../modules/projectController.js';\r\nexport default {\r\n  components: {\r\n    addProject,\r\n    addGroup\r\n  },\r\n\tdata() {\r\n\t\treturn {\r\n      projSnackbar: false,\r\n      groupSnackbar: false,\r\n\t\t}\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      user: 'user',\r\n      groups: 'groups',\r\n      projects: 'projects',\r\n    })\r\n  },\r\n\tmethods: {\r\n    sortBy(prop) {\r\n        this.projects.sort((a, b) => a[prop] < b[prop] ? -1 : 1)\r\n      },\r\n    created() {\r\n      // Get projects here\r\n    },\r\n    findProject(id) {\r\n      const project = getProject(id);\r\n      console.log('creating projects...');\r\n      this.$store.commit('createProject', project);\r\n    },\r\n    findGroup(id) {\r\n      const group = getGroup(id);\r\n      console.log('creating groups...');\r\n      this.$store.commit('createGroup', group);\r\n    },\r\n\r\n  },\r\n}\r\n</script>\r\n\r\n<style>\r\n.project.completed{\r\n  border-left: 4px solid #3CD1C2;\r\n}\r\n.project.ongoing{\r\n  border-left: 4px solid orange;\r\n}\r\n.project.overdue{\r\n  border-left: 4px solid tomato;\r\n}\r\n.v-chip.completed {\r\n  background: #3CD1C2;\r\n}\r\n.v-chip.ongoing {\r\n  background: orange;\r\n}\r\n.v-chip.overdue {\r\n  background: tomato;\r\n}\r\n</style>"]}]}